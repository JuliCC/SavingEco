      <div class="col-md-8">
        <div class="row">
          <%= form_for @contact do |f| %>
          <div class="col-sm-6 form-group">
          	<%= f.label :name %>
          	<%= f.text_field :name %>
          </div>
  
          <div class="col-sm-6 form-group">
          	<%= f.label :email %>
          	<%= f.text_field :email %>
          </div>



          <div class="col-sm-6 form-group">
          	<%= f.label :phone %>
          	<%= f.text_field :phone %>
          </div>

          <div class="col-sm-6 form-group">
          	<%= f.label :subject %>
          	<%= f.text_field :subject %>
          </div>

          <div>
          	<%= f.label :content %>
          	<%= f.text_area :content %>
          </div>

          <div class="row">
            <div class="col-md-12 form-group">
          	  <%= f.submit 'Send' , class:"btn pull-right" %>
              <% end %>
            </div>
          </div>

        </div>
      </div>
    </div>
