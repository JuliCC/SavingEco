<section id="contact">
	<div class="container">
    <h1 class="text-center">Contact</h1>
    <h3 class="text-center text-muted">bla bla bla bla </h3>

    <div class="row">
      <div class="col-md-4">
        <h3>Datos de contacto</h3>
        <h4><i class="fa fa-female fa-contact" aria-hidden="true"></i> Julieth Cardona C.</h4>
        <h4><i class="fa fa-mobile fa-contact" aria-hidden="true"></i> 310 440 3341</h4>
        <h4><i class="fa fa-envelope fa-contact" aria-hidden="true"></i> juli-c.c@hotmail.es</h4>
        <h4><i class="fa fa-map-marker fa-contact" aria-hidden="true"></i> Medellín, Colombia</h4>
      </div>

      <div class="col-md-8">
        <div class="form-group" >
        <%= form_for @contact do |f| %>
        <%= f.label :name, :class => "control-label col-lg-2" %>
        <%= f.text_field :name, :class => "form-control" %>
        </div>

        <div class="col-sm-8 form-group" >
        <%= f.label :email, :class => "control-label" %>
        <%= f.text_field :email, :class => "form-control" %>
        </div>
        
        <div class="col-sm-4 form-group" >
      	<%= f.label :phone, :class => "control-label" %>
      	<%= f.text_field :phone, :class => "form-control" %>
        </div>
        
        
        <div class="form-group">
      	<%= f.label :content, :class => "control-label col-lg-2" %>
      	<%= f.text_area :content, size:"50x5", :class => "form-control" %>
        </div>
        
        <div class="form-group">
          <div class="col-lg-offset-2 col-lg-10">
    	  <%= f.submit 'Send', :class => "btn pull-right" %>
        <% end %>
          </div>
        </div> 
      </div>
    </div>
  </div> 
</section>

